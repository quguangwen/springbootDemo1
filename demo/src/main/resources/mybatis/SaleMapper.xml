<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.SaleMapper">
    <delete id="deleteSaleByNO" parameterType="com.demo.entity.Sale">
        delete from app_user_token where sales_no = #{saleNo}
    </delete>

    <delete id="deleteAllDevice">
        delete from app_user_token;
    </delete>
</mapper>